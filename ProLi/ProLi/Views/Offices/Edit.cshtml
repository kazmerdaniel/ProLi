@model ProLi.Models.Office

@{
    ViewData["Title"] = "Edit";
}

<h1>Jogviszony szerkesztése</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="OfficePost" class="control-label"></label>
                <input asp-for="OfficePost" class="form-control" />
                <span asp-validation-for="OfficePost" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OfficeStart" class="control-label"></label>
                <input asp-for="OfficeStart" class="form-control" />
                <span asp-validation-for="OfficeStart" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OfficeEnd" class="control-label"></label>
                <input asp-for="OfficeEnd" class="form-control" />
                <span asp-validation-for="OfficeEnd" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OfficeName1" class="control-label"></label>
                <input asp-for="OfficeName1" class="form-control" />
                <span asp-validation-for="OfficeName1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OfficeName2" class="control-label"></label>
                <input asp-for="OfficeName2" class="form-control" />
                <span asp-validation-for="OfficeName2" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OfficeName3" class="control-label"></label>
                <input asp-for="OfficeName3" class="form-control" />
                <span asp-validation-for="OfficeName3" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OfficeAddress" class="control-label"></label>
                <input asp-for="OfficeAddress" class="form-control" />
                <span asp-validation-for="OfficeAddress" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OfficeEmail" class="control-label"></label>
                <input asp-for="OfficeEmail" class="form-control" />
                <span asp-validation-for="OfficeEmail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OfficePhone" class="control-label"></label>
                <input asp-for="OfficePhone" class="form-control" />
                <span asp-validation-for="OfficePhone" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="People_Id" class="control-label"></label>
@*                <input asp-for="People_Id" class="form-control" />
*@                <select asp-for="People_Id" class="form-control" asp-items="ViewBag.People_Id"></select>
                <span asp-validation-for="People_Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Mentés" class="btn btn-secondary " />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="javascript:history.back()">Vissza</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
